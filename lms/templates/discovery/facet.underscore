<h3 class="header-facet">
    <%- gettext(displayName) %>
</h3>


<% if (name == "org"){ %>
    <select style="width:100%" data-facet="<%- name %>" class="facet-list collapse">
        <option value="">전체</option>
        <%= HtmlUtils.ensureHtml(optionsHtml) %>
    </select>
<% } %>

<% if (name != "org"){ %>
    <ul style="width:100%" data-facet="<%- name %>" class="facet-list collapse">
        <%= HtmlUtils.ensureHtml(optionsHtml) %>
    </ul>
<% } %>
