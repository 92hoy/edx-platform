<article class="course <%- status %> grid-view" role="region" aria-label="<%- content.display_name %>">
    <a href="edxapp://course_info?path_id=course/<%- course %>">
        <header class="course-image">
            <div class="cover-image">
                <span class="univ-logo">
                    <!--
                    <img src="/static/images/univ/logo02_<%- org %>.png" alt=""  onerror="this.src='/static/images/no_images.png'">
                    -->
                </span>
                <img src="<%- image_url %>" alt="<%- content.display_name %> <%- content.number %>"  onerror="this.src='/static/images/no_images_large.png'"/>
                <div class="learn-more" aria-hidden="true"><%- gettext("LEARN MORE") %></div>
            </div>
        </header>
        <div class="course-info" aria-hidden="true">
            <h2 class="course-name">
                <span class="course-status over"><%- gettext(status) %></span>
                <span class="course-title"><%- content.display_name %></span>
            </h2>
            <div class="course-date" aria-hidden="true">
                <time><%- start %></time></span>
                <% if (end){ %>
                ~ </strong><time><%- end %></time>
                <% } %>
            </div>
            <div class="course-date" aria-hidden="true" style="position: absolute; bottom: 5px;">
                <time><%- gettext(org) %></time>
            </div>
        </div>
        <div class="sr">
            <ul>
                <li><%- org %></li>
                <li><%- content.number %></li>
                <li><%- gettext("Starts") %><time itemprop="startDate" datetime="<%- start %>"><%- start %></time></li>
            </ul>
        </div>
    </a>
</article>



<div class="discovery-card course-card shadow verified list-view" style="width : 99% !important; margin: 0 1% 25px; height: auto !important; min-height: 50px !important; padding: 0; align-items: center; margin-bottom: 7px !important; display: flex; -webkit-box-shadow: 1px 2px 5px #ccc; display: none;">
    <a style="width:400px; display:inline-block; margin-left:10px;" href="edxapp://course_info?path_id=course/bioethics-law-medicine-ethics-harvardx-hls4x-3" class="course-link"><h3 style="font-size: 1.25em;" class="title-heading" lang="en"><%- content.display_name %></h3></a>

    <div class="org-title" style="margin: 10px; display:inline-block; width:140px"><time><%- gettext(org) %></time></div>

    <div class="card-type" style="width:60px; display:inline-block; margin-right:10px;">Course</div>

    <div class="extra" style="width:200px; display:inline-block;" itemprop="startDate" content="Self-Paced" data-timestamp="1512417600"><time><%- start %></time></span>
                    <% if (end){ %>
                    ~ </strong><time><%- end %></time>
                    <% } %></div>

</div>